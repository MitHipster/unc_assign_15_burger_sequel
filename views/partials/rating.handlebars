<h5>{{{parameter.rating_stars}}} | {{parameter.rating_desc}} <span class="count">({{parameter.Burgers.length}})</span></h5>
<div class="row">
  {{#each parameter.Burgers}}
    <div class="col s12 m6 l4">
      <div class="card">
        <div class="card-content">
          <span class="card-title activator grey-text text-darken-4">{{~this.burger_name~}}<i class="material-icons right">more_vert</i></span>
          <p>{{~this.rest_name~}}</p>
          <form action="/{{this.id}}?_method=DELETE" method="POST">
            <input type="hidden" name="devoured" value="true">
            <button type="submit" class="waves-effect waves-light btn">Delete It</button>
          </form>
        </div>
        <div class="card-reveal">
          <span class="card-title grey-text text-darken-4">Description<i class="material-icons right">close</i></span>
          <p>{{~this.burger_desc~}}</p>
        </div>
      </div>
    </div>
  {{/each}}
</div>
